page{
  height: 100%;
}
.container{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  
}
.setprize{
  display: flex;
  justify-content:center;
  margin-top: 80rpx;
  width: 100%;
  align-items: baseline;
}
.input{
  margin: 0 0 3px 18px;
  /* min-height: 0.8rem;
  height: 30%; */
  width: 60%;
  border:#558d92 solid 5rpx;
}
form{
  display: flex;
  flex-direction: row;
}
button{
  background-color:hsl(39, 64%, 65%);
  display: flex;
  justify-content: center;
  margin-top: 60rpx;
  color:#fff;
  
}
.background{
  
  width: 100%;
  height: 100%;
  position: fixed;
  background-size: 100% 100%;
  z-index: -1;
}